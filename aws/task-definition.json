{
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "family": "LearnJenkinsApp-TaskDefinition-Prod",
  "containerDefinitions": [
    {
      "name": "learnjenkinsapp",
      "image": "arn:aws:ecs:eu-central-1:798680949553:task-definition/LearnJenkinsApp-TaskDefinition-Prod:17",
      "portMappings": [{
        "name": "nginx-80-tcp",
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp",
        "appProtocol": "http"

      }],
      "essential": true
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::798680949553:role/ecsTaskExecutionRole"
}